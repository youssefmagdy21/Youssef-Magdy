/*
 * to populate data to the section when used inside this template
 * define a setting then refrence that setting with its id here in the template
 * then pass a valid value to it
 * settings: {
 *     "settingID": value }
 */
{
  "sections": {
    "banner": {
      "type": "custom-banner-section",
      "settings": {}
    },
    "grid": {
      "type": "custom-grid-section",
      "settings": {}
    }
  },
  "order": ["banner", "grid"]
}
